{
    "manifest_version": 3,
    "name": "ALX_MultiTone-Magic",
    "version": "1.0",
    "description": "Change webpage colors ",
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "matches": ["https://intranet.alxswe.com/*"],
            "js": ["public/popup.js"]
        }
    ],
    "action": {
        "default_title": "ALX Intranet themes",
      "default_popup": "public/index.html"
    }
  }